{
  "name": "lalalili/text-to-speech",
  "description": "Text-to-speech driver package for Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Lalalili",
      "email": "dev@example.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-mbstring": "*",
    "google/cloud-text-to-speech": "^1.0",
    "illuminate/bus": "^12.0|^11.0",
    "illuminate/cache": "^12.0|^11.0",
    "illuminate/contracts": "^12.0|^11.0",
    "illuminate/database": "^12.0|^11.0",
    "illuminate/filesystem": "^12.0|^11.0",
    "illuminate/queue": "^12.0|^11.0",
    "illuminate/support": "^12.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^10.0",
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "laravel/pint": "^1.14"
  },
  "autoload": {
    "psr-4": {
      "Lalalili\\TextToSpeech\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lalalili\\TextToSpeech\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lalalili\\TextToSpeech\\TextToSpeechServiceProvider"
      ],
      "aliases": {
        "TextToSpeech": "Lalalili\\TextToSpeech\\Facades\\TextToSpeech"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/pest",
    "format": "vendor/bin/pint"
  }
}
